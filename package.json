{
  "name": "electron-quick-start",
  "version": "1.0.13",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=out --icon=assets/app.ico",
    "package-mac": "electron-packager . --asar --overwrite --platform=darwin --arch=x64 --out=out ",
    "build": "node ./script/installer.js"
  },
  "author": "常州完美维度科技有限公司",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.1.2",
    
    "electron-packager": "^15.0.0",
    "electron-winstaller": "^4.0.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-log": "^4.2.2"
  }
}
